{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "content_scripts": [ {
      "css": [ "dist/css/content.css", "dist/css/standup.css" ],
      "js": [ "dist/js/better-jira.js" ],
      "matches": [ "https://*.atlassian.net/*" ]
   } ],
